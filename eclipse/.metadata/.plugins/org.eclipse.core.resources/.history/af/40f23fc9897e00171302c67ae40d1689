package com.TominoCZ.FBP;

import javax.vecmath.Vector2f;
import javax.vecmath.Vector3f;

public class IBakedQuadF {

	public Vector3f pos;
	public Vector3f normals;
	
	public Vector2f UV;
	
	public IBakedQuadF(float x, float y, float z, float U, float V, float nX, float nY, float nZ) {
		pos = new Vector3f(x, y, z);
		normals = new Vector3f(nX, nY, nZ);
		
		UV = new Vector2f(U, V);
	}
}
